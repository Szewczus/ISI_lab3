<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TextArea?>
<VBox fx:controller="sample.Controller" xmlns:fx="http://javafx.com/fxml" prefHeight="431.0" prefWidth="409.0" spacing="20.0" >
    <children>
        <HBox spacing="20">
            <children>
                <Button style="-fx-font: NORMAL 15 Tahoma;" text="Wczytaj dane z TXT"   GridPane.columnIndex="0" GridPane.rowIndex="0" fx:id="wczytaj_txt"/>
                <Button style="-fx-font: NORMAL 15 Tahoma;" text="Wczytaj dane z XML"  GridPane.columnIndex="1" GridPane.rowIndex="0"  fx:id="wczytaj_xml"/>
                <Button style="-fx-font: NORMAL 15 Tahoma;" text="Zapisz dane do TXT"   GridPane.columnIndex="2" GridPane.rowIndex="0" fx:id="zapisz_txt"/>
                <Button style="-fx-font: NORMAL 15 Tahoma;" text="Zapisz dane do XML"  GridPane.columnIndex="3" GridPane.rowIndex="0"  fx:id="zapisz_xml"/>
            </children>
        </HBox>

        <TableView fx:id="tableView" GridPane.columnIndex="0" GridPane.rowIndex="1" style="-fx-font: NORMAL 10 Tahoma;">
        </TableView>


        <GridPane fx:id="miejsce_na_dane">
        </GridPane>

        <GridPane fx:id="miejsce_na_przyciski">
            <children>
                <Button style="-fx-font: NORMAL 15 Tahoma;" text="Dodaj nowy wiersz"   GridPane.columnIndex="0" GridPane.rowIndex="4" fx:id="dodaj"/>
            </children>
        </GridPane>
    </children>

</VBox>

